<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tcs.sgv.dcps.valueobject">

	<class name="UpdateFrnNo" table="HST_FRN_DTLS">

		<id name="frnhstid" type="long">
			<column name="FRN_HST_ID" />
			<generator class="assigned" />
		</id>

		<property name="fileName" type="string">
			<column name="FILENAME" />
		</property>

		<property name="empAmount" type="string">
			<column name="EMP_AMOUNT" />
		</property>

		<property name="emplyrAmount" type="string">
			<column name="EMPLYR_AMOUNT" />
		</property>

		<property name="oldFrn" type="string">
			<column name="OLD_FRN" />
		</property>

		<property name="newFrnNo" type="string">
			<column name="NEW_FRN_NO" />
		</property>

		<property name="remarks" type="string">
			<column name="REMARKS" />
		</property>

		<property name="createdDate" type="timestamp">
			<column name="CREATED_DATE" />
		</property>

		<property name="updatedDate" type="timestamp">
			<column name="UPDATED_DATE" />
		</property>


	</class>
</hibernate-mapping>