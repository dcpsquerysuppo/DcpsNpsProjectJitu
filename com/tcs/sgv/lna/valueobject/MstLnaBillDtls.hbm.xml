<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tcs.sgv.lna.valueobject">
  <class name="com.tcs.sgv.lna.valueobject.MstLnaBillDtls" table="MST_LNA_BILL_DTLS" >
  	<id name="billDtlsId" type="long">
            <column name="BILL_DTLS_ID"  />
            <generator class="assigned" />
     </id>
     <property name="locationCode" type="string">
            <column name="LOCATION_CODE"  />
  	</property>
    
    <property name="billAmount" type="long">
            <column name="BILL_AMOUNT"  />
  	</property>
  	<property name="billGeneratedDate" type="timestamp">
            <column name="BILL_GENERATED_DATE"  />
  	</property>
  	<property name="advacnceType" type="long">
            <column name="ADVANCE_TYPE"  />
  	</property>
     <property name="statusFlag" type="long">
            <column name="STATUS_FLAG"  />
  	</property>
  	<property name="voucherNo" type="string">
            <column name="VOUCHER_NO" />
 	</property>
 	<property name="voucherDate" type="timestamp">
            <column name="VOUCHER_DATE" />
 	</property>
 	<property name="orderNo" type="string">
			<column name="ORDER_NO" length="50" />
	</property>
	<property name="orderDate" type="timestamp">
			<column name="ORDER_DATE" length="30" />
	</property>
  	<property name="createdDate" type="timestamp">
            <column name="CREATED_DATE"  />
	</property>
	<property name="createdUserId" type="long">
            <column name="CREATED_USER_ID" />
	</property>
	<property name="createdPostId" type="long">
            <column name="CREATED_POST_ID"  />
	</property>
	<property name="updatedDate" type="timestamp">
            <column name="UPDATED_DATE"/>
	</property>
	<property name="updatedUserId" type="long">
            <column name="UPDATED_USER_ID"/>
	</property>
	<property name="updatedPostId" type="long">
            <column name="UPDATED_POST_ID"/>
	</property>  
   </class>
</hibernate-mapping>
