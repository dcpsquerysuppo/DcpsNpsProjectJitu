<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

<class name="com.tcs.sgv.gpf.valueobject.MstGpfBillDtls" table="MST_GPF_BILL_DTLS">
  		<id name="billDtlsId" type="long">
			<column name="BILL_DTLS_ID" precision="30" scale="0" />
			<generator class="assigned" />
		</id>
		
		<property name="locationCode" type="string">
            <column name="LOCATION_CODE"  />
	  	</property>
	    
	    <property name="gpfAccNo" type="string">
	            <column name="GPF_ACC_NO"  />
	  	</property>
	  	
	  	<property name="transactionId" type="string">
	            <column name="TRANSACTION_ID"  />
	  	</property>
	  	
	  	<property name="billGeneratedDate" type="timestamp">
	            <column name="BILL_GENERATED_DATE"  />
	  	</property>
	  
	  	<property name="advacnceType" type="string">
	            <column name="ADVANCE_TYPE"  />
	  	</property>
	    
	    <property name="statusFlag" type="integer">
	            <column name="STATUS_FLAG"  />
	  	</property>
	  	
	  	<property name="voucherNo" type="string">
	            <column name="VOUCHER_NO" />
	 	</property>
	 	
	 	<property name="voucherDate" type="timestamp">
	            <column name="VOUCHER_DATE" />
	 	</property>
	 	
	 	<property name="orderNo" type="string">
				<column name="ORDER_NO" length="50" />
		</property>
		
		<property name="orderDate" type="timestamp">
				<column name="ORDER_DATE" length="30" />
		</property>
	  	
	  	<property name="createdDate" type="timestamp">
	            <column name="CREATED_DATE"  />
		</property>
		
		<property name="createdUserId" type="long">
	            <column name="CREATED_USER_ID" />
		</property>
		
		<property name="createdPostId" type="long">
	            <column name="CREATED_POST_ID"  />
		</property>
		
		<property name="updatedDate" type="timestamp">
	            <column name="UPDATED_DATE"/>
		</property>
		
		<property name="updatedUserId" type="long">
	            <column name="UPDATED_USER_ID"/>
		</property>
		
		<property name="updatedPostId" type="long">
	            <column name="UPDATED_POST_ID"/>
		</property>
		<property name="openingBalance" type="double">
            <column name="OPENING_BALC"  />
  		</property>
  		<property name="regularSubscription" type="double">
            <column name="REGULAR_SUB"  />
  		</property>
  		<property name="advanceRecovery" type="double">
            <column name="ADVANCE_REC"  />
  		</property>
  		<property name="advanceSanctioned" type="double">
            <column name="ADVANCE_SANC"  />
  		</property>
  		<property name="billNo" type="string">
            <column name="bill_no"  />
  		</property>
	</class>

</hibernate-mapping>